<h1 align="center">All Categories</h1>


<div class="row">
  <ul class="list-group col-md-4 col-md-offset-4">
    <% @categories.each do |category| %>
    <li class="list-group-item">
      <%= link_to "#{category.name}", category_path(category) %>
    </li>
    <% end %>
  </ul>
</div>

<div align="center">
  <%= will_paginate %>
</div>
